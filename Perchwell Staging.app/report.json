{
    "report_types":[
        {
            "title":"CMA",
            "subtypes":[
                {
                    "title":"Sales",
                    "value":{
                        "report_key":"sales_cma",
                        "api_key":"cma"
                    }
                },
                {
                    "title":"Rentals",
                    "value":{
                        "report_key":"rental_cma",
                        "api_key":"cma"
                    }
                }
            ]
        }
    ],
    "report_data":{
        "sales_cma":{
            "title":"CMA",
            "forms":{
                "listing_table":{
                    "status_groups":true,
                    "allows_editing":true,
                    "has_subject_property":true,
                    "has_tag_selection":true,
                    "permitted_record_types":[
                        "sales",
                        "rentals"
                    ],
                    "instructions":"Click a listing to customize the show sheet. Drag to order listings."
                },
                "subject_property":{
                    "title":"Add Subject Property",
                    "elements":[
                        {
                            "element_class":"text",
                            "ui_type":"single-line",
                            "label":"Address",
                            "placeholder":"Enter the building address",
                            "format":{
                                "type":"string"
                            },
                            "required":false,
                            "key":"address",
                            "max":250
                        },
                        {
                            "element_class":"text",
                            "ui_type":"single-line",
                            "label":"Unit",
                            "placeholder":"Enter the unit number",
                            "format":{
                                "type":"string"
                            },
                            "required":true,
                            "key":"unit_number",
                            "max":25
                        },
                        {
                            "element_class":"text",
                            "ui_type":"single-line",
                            "label":"Price",
                            "placeholder":"$",
                            "format":{
                                "type":"money",
                                "decimal_places":2,
                                "prefix":"$"
                            },
                            "required":true,
                            "key":"listing_price"
                        },
                        {
                            "element_class":"text",
                            "ui_type":"single-line",
                            "label":"Rooms",
                            "placeholder":"Number of total rooms",
                            "format":{
                                "type":"integer"
                            },
                            "key":"num_rooms",
                            "max":99
                        },
                        {
                            "element_class":"text",
                            "ui_type":"single-line",
                            "label":"Bathrooms",
                            "placeholder":"Use .5 for half baths",
                            "format":{
                                "type":"float",
                                "decimal_places":2
                            },
                            "key":"num_bathrooms",
                            "max":99
                        },
                        {
                            "element_class":"text",
                            "ui_type":"single-line",
                            "label":"Bedrooms",
                            "placeholder":"# of bedrooms",
                            "format":{
                                "type":"integer"
                            },
                            "key":"num_bedrooms",
                            "max":99
                        },
                        {
                            "element_class":"text",
                            "ui_type":"single-line",
                            "label":"Square Feet",
                            "placeholder":"Property square footage",
                            "format":{
                                "type":"integer"
                            },
                            "key":"sqft",
                            "max":99999
                        },
                        {
                            "element_class":"text",
                            "ui_type":"single-line",
                            "label":"RE Taxes",
                            "placeholder":"$/month",
                            "format":{
                                "type":"money",
                                "decimal_places":0,
                                "prefix":"$"
                            },
                            "key":"property_taxes",
                            "listing_types":[
                                "sale"
                            ]
                        },
                        {
                            "element_class":"text",
                            "ui_type":"single-line",
                            "label":"Maintenance/Common Charges",
                            "placeholder":"$/month",
                            "format":{
                                "type":"money",
                                "decimal_places":0,
                                "prefix":"$"
                            },
                            "key":"maint_cc",
                            "listing_types":[
                                "sale"
                            ]
                        },
                        {
                            "element_class":"radio",
                            "ui_type":"collapsible",
                            "label":"Property Type",
                            "placeholder":"Select a property type",
                            "selections":[
                                {
                                    "label":"Condo",
                                    "value":540
                                },
                                {
                                    "label":"Co-op",
                                    "value":550
                                },
                                {
                                    "label":"Condop",
                                    "value":530
                                },
                                {
                                    "label":"Single Family Townhouse",
                                    "value":560
                                },
                                {
                                    "label":"Mixed-Use Townhouse",
                                    "value":570
                                },
                                {
                                    "label":"Multi-Family Townhouse",
                                    "value":580
                                },
                                {
                                    "label":"Rental",
                                    "value":400
                                }
                            ],
                            "key":"property_type_code"
                        },
                        {
                            "element_class":"boolean",
                            "label":"Building Doorman",
                            "key":"building_doorman"
                        },
                        {
                            "element_class":"boolean",
                            "label":"Outdoor Space",
                            "key":"outdoor_y_n"
                        },
                        {
                            "element_class":"radio",
                            "ui_type":"collapsible",
                            "label":"Outdoor",
                            "placeholder":"Select a type",
                            "selections":[
                                {
                                    "label":"Terrace",
                                    "value":"Terrace"
                                },
                                {
                                    "label":"Balcony",
                                    "value":"Balcony"
                                }
                            ],
                            "key":"outdoor_t_or_b"
                        },
                        {
                            "element_class":"text",
                            "ui_type":"single-line",
                            "label":"Days on Market",
                            "placeholder":"# of days",
                            "format":{
                                "type":"integer"
                            },
                            "key":"days_on_market"
                        },
                        {
                            "element_class":"text",
                            "ui_type":"single-line",
                            "label":"Building Size",
                            "placeholder":"10 x 10",
                            "format":{
                                "type":"string"
                            },
                            "key":"building_size"
                        },
                        {
                            "element_class":"text",
                            "ui_type":"single-line",
                            "label":"Lot Size",
                            "placeholder":"10 x 10",
                            "format":{
                                "type":"string"
                            },
                            "key":"lot_size"
                        }
                    ]
                },
                "additional_options":{
                    
                },
                "generate_report":{
                    
                }
            }
        }
    }
}
